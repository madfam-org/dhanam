{
  "name": "@dhanam/mobile",
  "version": "0.1.0",
  "private": true,
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "expo start --dev-client",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "preview": "expo install --fix && expo start --dev-client",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf .expo node_modules"
  },
  "dependencies": {
    "@dhanam/shared": "workspace:*",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.5.1",
    "@react-navigation/bottom-tabs": "^7.12.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.11.0",
    "@reduxjs/toolkit": "^2.10.1",
    "@tanstack/react-query": "^5.90.10",
    "axios": ">=1.13.3",
    "expo": "~54.0.33",
    "expo-auth-session": "~5.5.2",
    "expo-camera": "~15.0.10",
    "expo-crypto": "~13.0.2",
    "expo-device": "~8.0.10",
    "expo-font": "~14.0.11",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~8.0.9",
    "expo-local-authentication": "~17.0.8",
    "expo-notifications": "~0.32.13",
    "expo-router": "~3.5.14",
    "expo-secure-store": "~13.0.1",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~55.0.9",
    "react": "19.2.4",
    "react-native": "0.83.1",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "4.21.0",
    "react-native-svg": "15.15.1",
    "react-redux": "^9.2.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@dhanam/config": "workspace:*",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.2.10",
    "@types/react-native": "^0.73.0",
    "eslint": "^8.57.0",
    "jest": "^30.2.0",
    "jest-expo": "~51.0.2",
    "typescript": "~5.9.3"
  },
  "jest": {
    "preset": "jest-expo",
    "passWithNoTests": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  }
}